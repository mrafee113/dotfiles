    {
      "before": [
        "<leader>",
        "n"
      ],
      "commands": [
        ":set relativenumber!"
      ]
    },
    {
      "before": [
        "<leader>",
        "h",
        "h"
      ],
      "commands": [
        ":noh",
        ":call clearmatches()"
      ]
    },
    {
